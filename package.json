{
  "name": "pyenv-action",
  "version": "1.0.0",
  "private": true,
  "description": "Setup python action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "release": "ncc build src/pyenv-action.ts && git add -f dist/",
    "test": "jest --runInBand",
    "test-watch": "jest --runInBand --detectOpenHandles --bail --watch",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielfalcao/pyenv-action.git"
  },
  "keywords": [
    "actions",
    "python",
    "pyenv",
    "node",
    "setup"
  ],
  "author": "Gabriel Falc√£o",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.4",
    "@actions/github": "2.2.0",
    "@actions/exec": "1.0.4",
    "@actions/io": "1.0.2",
    "@actions/tool-cache": "1.6.0",
    "semver": "7.3.2"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/js-yaml": "3.12.5",
    "@types/nock": "11.1.0",
    "@types/node": "12.12.42",
    "@types/semver": "7.3.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "3.1.0",
    "jest": "26.1.0",
    "jest-circus": "26.1.0",
    "prettier": "1.19.1",
    "ts-jest": "26.1.2",
    "typescript": "3.9.6"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "make release"
    }
  }
}
